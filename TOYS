import java.io.*;
import java.util.*;

class Toy {
    int id;
    String name;
    int quantity;
    int weight;

    public Toy(int id, String name, int quantity, int weight) {
        this.id = id;
        this.name = name;
        this.quantity = quantity;
        this.weight = weight;
    }

    public void updateWeight(int newWeight) {
        this.weight = newWeight;
    }
}

